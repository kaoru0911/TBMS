project 'TBMS.xcodeproj'

# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'TBMS' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for TBMS
  pod 'GoogleMaps'
  pod 'GooglePlaces'
  pod 'GooglePlacePicker'
  pod'Alamofire', '~> 4.4.0'
  pod 'AlamofireImage'
  pod 'SwiftyJSON'
  pod 'FacebookCore'
  pod 'FacebookLogin'
  pod 'FacebookShare'
  
end
